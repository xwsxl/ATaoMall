-(void)getDatas
{
    
    
    AFHTTPRequestOperationManager *manager = [AFHTTPRequestOperationManager manager];
    
    manager.responseSerializer = [AFHTTPResponseSerializer serializer];
    
    NSString *url = [NSString stringWithFormat:@"%@getAllOrders_mob.shtml",URL_Str];
    
    NSDictionary *dic = @{@"sigen":self.sigen,@"exchange_id":page};
    
    //    NSDictionary *dic=nil;
    
    NSLog(@"kkkkk%@",page);
    
    [manager POST:url parameters:dic success:^(AFHTTPRequestOperation *operation, id responseObject) {
        NSString *codeKey = [SecretCodeTool getDesCodeKey:operation.responseString];
        NSString *content = [SecretCodeTool getReallyDesCodeString:operation.responseString];
        
        
        
        
        if (codeKey && content) {
            NSString *xmlStr = [DesUtil decryptUseDES:content key:codeKey];
            xmlStr = [xmlStr stringByReplacingOccurrencesOfString:@"&lt;" withString:@"<"];
            xmlStr = [xmlStr stringByReplacingOccurrencesOfString:@"&gt;" withString:@">"];
            
            NSLog(@"xmlStr===%@",xmlStr);
            
            
            NSData *data = [[NSData alloc] initWithData:[xmlStr dataUsingEncoding:NSUTF8StringEncoding]];
            
            
            NSDictionary *dic = [NSJSONSerialization JSONObjectWithData:data options:NSJSONReadingMutableContainers error:nil];
            
            NSLog(@"全部订单***=%@",dic);
            
            //清空数据源
//            if ([page isEqualToString:@"0"]) {
//                
//                [_datas removeAllObjects];
//                
//            }
            for (NSDictionary *dict1 in dic) {
                string10 = dict1[@"totalCount"];
                
                for (NSDictionary *dict2 in dict1[@"exchangelist"]) {
                    AllDanModel *model=[[AllDanModel alloc] init];
                    
                    model.attribute_str=dict2[@"attribute_str"];
                    model.id=dict2[@"id"];
                    model.logo=dict2[@"logo"];
                    model.name=dict2[@"name"];
                    model.number=dict2[@"number"];
                    model.orderno=dict2[@"orderno"];
                    model.pay_integer=dict2[@"pay_integer"];
                    model.pay_maney=dict2[@"pay_maney"];
                    model.scopeimg=dict2[@"scopeimg"];
                    model.status=dict2[@"status"];
                    model.storename=dict2[@"storename"];
                    model.totalfreight=dict2[@"totalfreight"];
                    model.payintegral=dict2[@"payintegral"];
                    model.paymoney=dict2[@"paymoney"];
                    model.type=dict2[@"type"];
                    model.mid=dict2[@"mid"];
                    
                    [_datas addObject:model];
                    
                    if (_datas.count%12==0) {
                        NSLog(@"====%@",model.id);
                         stringID=model.id;
//                        footer.hidden=NO;
                        
                    }else{
                        
                        //隐藏点击加载更多
//                        footer.hidden=YES;
                        
                    }
                }
            }
            if (_datas.count == 0) {
                
                [self initview];
                
            }else{
                
                _lable.hidden = YES;
                
            }
            
            if (_datas.count%12==0) {
                footer.hidden=NO;
                [footer.loadMoreBtn setTitle:@"点击加载更多" forState:0];
                [footer.loadMoreBtn setTitleColor:[UIColor colorWithRed:91/255.0 green:150/255.0 blue:255/255.0 alpha:1.0] forState:0];
                footer.loadMoreBtn.enabled=YES;
                
            }else if (_datas.count == [string10 integerValue]){
                
                footer.moreView.hidden=YES;
                [footer.loadMoreBtn setTitle:@"暂无更多数据" forState:0];
                [footer.loadMoreBtn setTitleColor:[UIColor colorWithRed:102/255.0 green:102/255.0 blue:102/255.0 alpha:1.0] forState:0];
                footer.loadMoreBtn.enabled=NO;
                
                
            }else{
                
                //隐藏点击加载更多
                footer.hidden=YES;
                
            }
            
            
            
            [_myTableView reloadData];
        }
        
        
    } failure:^(AFHTTPRequestOperation *operation, NSError *error) {
        
        [WKProgressHUD popMessage:@"网络异常" inView:self.view duration:1.5 animated:YES];
        NSLog(@"%@",error);
    }];
}
-(void)initview{
    
    _lable = [[UILabel alloc]initWithFrame:CGRectMake(0,[UIScreen mainScreen].bounds.size.height/10 , [UIScreen mainScreen].bounds.size.width, 80)];
    _lable.text = @"暂无更多数据";
    _lable.textColor = [UIColor lightGrayColor];
    _lable.textAlignment = NSTextAlignmentCenter;
    self.view.backgroundColor = [UIColor colorWithRed:239/255.0 green:239/255.0 blue:244/255.0 alpha:1];
    [self.view addSubview:_lable];
    
}
-(void)initTableView
{
    //tableView
    _myTableView = [[UITableView alloc] initWithFrame:CGRectMake(0, 0, [UIScreen mainScreen].bounds.size.width, [UIScreen mainScreen].bounds.size.height-105) style:UITableViewStyleGrouped];
    _myTableView.dataSource = self;
    _myTableView.delegate = self;
    
    _myTableView.separatorStyle = UITableViewCellSeparatorStyleSingleLine;
    
    _myTableView.separatorColor = [UIColor colorWithRed:212.0f/255.0f green:212.0f/255.0f blue:212.0f/255.0f alpha:1.0];
    
    [self.view addSubview:_myTableView];
    
    [_myTableView registerNib:[UINib nibWithNibName:@"AllDingDanCell" bundle:nil] forCellReuseIdentifier:@"cell"];
    
    [_myTableView registerNib:[UINib nibWithNibName:@"WaiteGitCell" bundle:nil] forCellReuseIdentifier:@"cell1"];
    
    [_myTableView registerNib:[UINib nibWithNibName:@"WaitePayCell" bundle:nil] forCellReuseIdentifier:@"cell2"];
    
    [_myTableView registerNib:[UINib nibWithNibName:@"WaiteSendCell" bundle:nil] forCellReuseIdentifier:@"cell3"];
    
    [_myTableView registerNib:[UINib nibWithNibName:@"NoDingDanCell" bundle:nil] forCellReuseIdentifier:@"nocell"];
    //注册Footer
    
    [_myTableView registerNib:[UINib nibWithNibName:@"AllFooterView" bundle:nil] forHeaderFooterViewReuseIdentifier:@"footer"];
    
    [_myTableView registerNib:[UINib nibWithNibName:@"HomeLookFooter" bundle:nil] forHeaderFooterViewReuseIdentifier:@"header"];
    
    _refresh=[[DJRefresh alloc] initWithScrollView:_myTableView delegate:self];
    _refresh.topEnabled=YES;
    _refresh.bottomEnabled=NO;
}

