        JiaoYiDaiFuKuanViewController *vc=[[JiaoYiDaiFuKuanViewController alloc] init];
        
        vc.sigen=self.sigen;
        vc.orderno=model.orderno;
        vc.logo=model.logo;
        vc.storename=model.storename;
        
